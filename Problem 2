#include<stdio.h>
int main(){
    int n;
    printf("Enter a Number:- ");
    scanf("%d",&n);
    for(int i=1;i<=2;i++){
        n/=10;
        if(n==0){
            printf("The entered number is single digit.");break;
        }
        else{
              printf("The entered number is not single digit.");break;
        }
       
    }
    return 0;
}
